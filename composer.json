{
    "name": "kfriars/php-array-to-file",
    "description": "Convert a php array into a .php file that can be included",
    "keywords": [
        "kfriars",
        "php-array-to-file"
    ],
    "homepage": "https://github.com/spatie/php-array-to-file",
    "license": "MIT",
    "authors": [
        {
            "name": "Kurt Friars",
            "email": "kfriars@gmail.com",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "mockery/mockery": "^1.3",
        "phpunit/phpunit": "^8.0",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "Kfriars\\ArrayToFile\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kfriars\\ArrayToFile\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
